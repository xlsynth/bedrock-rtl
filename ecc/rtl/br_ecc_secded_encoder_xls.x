            let parity_4 = m[2+:u1] ^ m[6+:u1] ^ m[9+:u1] ^ m[12+:u1] ^ m[13+:u1] ^ m[15+:u1] ^ m[18+:u1] ^ m[21+:u1] ^ m[22+:u1] ^ m[25+:u1] ^ m[26+:u1] ^ m[27+:u1] ^ m[29+:u1] ^ m[30+:u1];
