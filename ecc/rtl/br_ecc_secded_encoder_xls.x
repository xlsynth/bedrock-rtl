            let parity_1 = m[0+:u1] ^ m[1+:u1] ^ m[2+:u1] ^ m[7+:u1] ^ m[8+:u1] ^ m[9+:u1] ^ m[10+:u1];
            let parity_2 = m[0+:u1] ^ m[4+:u1] ^ m[5+:u1] ^ m[6+:u1] ^ m[8+:u1] ^ m[9+:u1] ^ m[10+:u1];
            let parity_3 = m[1+:u1] ^ m[3+:u1] ^ m[5+:u1] ^ m[6+:u1] ^ m[7+:u1] ^ m[9+:u1] ^ m[10+:u1];
